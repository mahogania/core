datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Achievement {
  count           Int?
  createdAt       DateTime @default(now())
  flags           Int?
  id              String   @id @default(cuid())
  map             Int?
  points          Int?
  refAchievement  Int?
  requiredFaction Int?
  updatedAt       DateTime @updatedAt
}

model AccessRequirement {
  comment              String?
  completedAchievement Int?
  createdAt            DateTime @default(now())
  difficulty           Int?
  failureText          String?
  id                   String   @id @default(cuid())
  item                 Int?
  levelMax             Int?
  levelMin             Int?
  mapId                Int?
  questDone            Int?
  updatedAt            DateTime @updatedAt
}

model AchievementReward {
  body           String?
  createdAt      DateTime @default(now())
  id             String   @id @default(cuid())
  itemId         Int?
  mailTemplateId Int?
  sender         Int?
  subject        String?
  titleA         Int?
  titleH         Int?
  updatedAt      DateTime @updatedAt
}

model AchievementRewardLocale {
  body      String?
