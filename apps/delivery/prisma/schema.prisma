  barcode                String?
  baseAmount             Float?
  baseNetAmount          Float?
  baseNetRate            Float?
  basePriceListRate      Float?
  baseRate               Float?
  baseRateWithMargin     Float?
  batchNo                String?
  billedAmt              Float?
  brand                  String?
  conversionFactor       Float?
  costCenter             String?
  createdAt              DateTime  @default(now())
  creation               DateTime?
  customerItemCode       String?
  description            String?
  discountAmount         Float?
  discountPercentage     Float?
  dnDetail               String?
  docstatus              Int?
  expenseAccount         String?
  grantCommission        Boolean?
  hasItemScanned         Boolean?
  id                     String    @id @default(cuid())
  idx                    Int?
  image                  String?
  incomingRate           Float?
  installedQty           Float?
  isFreeItem             Boolean?
  itemCode               String?
  itemGroup              String?
  itemName               String?
  itemTaxRate            String?
  itemTaxTemplate        String?
  marginRateOrAmount     Float?
  marginType             String?
  materialRequest        String?
  materialRequestItem    String?
  modified               DateTime?
  modifiedBy             String?
  name                   String?
  netAmount              Float?
  netRate                Float?
  owner                  String?
  packedQty              Float?
  pageBreak              Boolean?
  parent                 String?
  parentfield            String?
  parenttype             String?
  pickListItem           String?
  priceListRate          Float?
  pricingRules           String?
  project                String?
  purchaseOrder          String?
  purchaseOrderItem      String?
  qty                    Float?
  qualityInspection      String?
  rate                   Float?
  rateWithMargin         Float?
  receivedQty            Float?
  returnedQty            Float?
  serialAndBatchBundle   String?
  serialNo               String?
  siDetail               String?
  soDetail               String?
  stockQty               Float?
  stockUom               String?
  stockUomRate           Float?
  targetWarehouse        String?
  totalWeight            Float?
  uom                    String?
  updatedAt              DateTime  @updatedAt
  useSerialBatchFields   Boolean?
  warehouse              String?
  weightPerUnit          Float?
  weightUom              String?
}

