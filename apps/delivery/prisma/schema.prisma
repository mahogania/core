model DeliveryStop {
  address          String?
  contact          String?
  createdAt        DateTime  @default(now())
  creation         DateTime?
  customer         String?
  customerAddress  String?
  customerContact  String?
  deliveryNote     String?
  details          String?
  distance         Float?
  docstatus        Int?
  emailSentTo      String?
  estimatedArrival DateTime?
  grandTotal       Float?
  id               String    @id @default(cuid())
  idx              Int?
  lat              Float?
  lng              Float?
  locked           Boolean?
  modified         DateTime?
  modifiedBy       String?
  name             String?
  owner            String?
  parent           String?
  parentfield      String?
  parenttype       String?
  uom              String?
  updatedAt        DateTime  @updatedAt
  visited          Boolean?
}

model DeliveryTrip {
  amendedFrom           String?
  company               String?
  createdAt             DateTime  @default(now())
  departureTime         DateTime?
  docstatus             Int?
  driver                String?
  driverAddress         String?
  driverEmail           String?
  driverName            String?
  emailNotificationSent Boolean?
  employee              String?
  id                    String    @id @default(cuid())
  idx                   Int?
  name                  String?
  namingSeries          String?
  owner                 String?
  status                String?
  totalDistance         Float?
  uom                   String?
  updatedAt             DateTime  @updatedAt
  vehicle               String?
}
