}

model CreatureQuestEnder {
  createdAt     DateTime @default(now())
  id            String   @id @default(cuid())
  quest         Int?
  updatedAt     DateTime @updatedAt
  verifiedBuild Int?
}

model CreatureLootTemplate {
  chance        Float?
  comment       String?
  createdAt     DateTime @default(now())
  entry         Int?
  groupId       Int?
  id            String   @id @default(cuid())
  item          Int?
  lootMode      Int?
  maxCount      Int?
  minCount      Int?
  questRequired Int?
  reference     Int?
  updatedAt     DateTime @updatedAt
}

model CreatureMovementOverride {
  chase                 Int?
  createdAt             DateTime @default(now())
  flight                Int?
  ground                Int?
  id                    String   @id @default(cuid())
  interactionPauseTimer Int?
  random                Int?
  rooted                Int?
  spawnId               Float?
  swim                  Int?
  updatedAt             DateTime @updatedAt
}

model CreatureTemplateResistance {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model CreatureFormations {
  angle      Float?
  createdAt  DateTime @default(now())
  dist       Float?
  groupAi    Int?
  id         String   @id @default(cuid())
  leaderGuid Float?
  memberGuid Float?
  point_1    Int?
  point_2    Int?
